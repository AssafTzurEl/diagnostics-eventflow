{
  "EventSources": [
    {
      "ProviderName": "Microsoft-ServiceFabric-Services",
      "Metrics": [
        {
          "EventName": "StatefulRunAsyncFailure",
          "Name": "StatefulRunAsyncFailure",
          "MetricValue": "1.0"
        },
        {
          "EventName": "StatefulRunAsyncInvocation",
          "Name": "StatefulRunAsyncInvocation",
          "MetricValue": "1.0"
        }
      ]
    },

    {
      "ProviderName": "MyCompany-AirTrafficControlApplication-Atc",
      "Metrics": [
        {
          "EventName": "ServiceRequestStop",
          "Name": "AtcServiceOperationDurationMsec",
          "MetricValueProperty": "durationMsec"
        },
        {
          "EventName": "FlightCompleted",
          "Name": "FlightCompletedTime",
          "MetricValueProperty": "flightDuration"
        }
      ],
      "Requests": [
        {
          "EventName": "ServiceRequestStop",
          "RequestNameProperty": "requestTypeName",
          "DurationProperty": "durationMsec",
          "IsSuccessProperty": "isSuccess"
        }
      ]
    }    
  ],

  "ApplicationInsightsSender": {
    "InstrumentationKey": "***REMOVED***"
  }
}
