{
  "EventSources": [
    {
      "ProviderName": "Microsoft-ServiceFabric-Services",
      "Metrics": [
        {
          "EventName": "StatefulRunAsyncFailure",
          "MetricName": "StatefulRunAsyncFailure",
          "MetricValue": "1.0"
        },
        {
          "EventName": "StatefulRunAsyncInvocation",
          "MetricName": "StatefulRunAsyncInvocation",
          "MetricValue": "1.0"
        }
      ]
    },

    {
      "ProviderName": "Microsoft-ServiceFabric-Actors",
      "Metrics": [
        {
          "EventName": "ActorMethodCallsWaitingForLock",
          "MetricName": "ActorMethodCallsWaitingForLock",
          "MetricValueProperty": "countOfWaitingMethodCalls"
        },
        {
          "EventName": "ActorActivated",
          "MetricName": "ActorActivated",
          "MetricValue": "1.0"
        }
      ]
    },

    {
      "ProviderName": "MyCompany-AirTrafficControlApplication-Airplanes",
      "Metrics": [
        {
          "EventName": "ActorMethodStop",
          "MetricName": "ActorMethodDurationMsec",
          "MetricValueProperty": "durationMsec"
        }
      ],
      "Requests": [
        {
          "EventName": "ActorMethodStop",
          "RequestNameProperty": "methodName",
          "DurationProperty": "durationMsec",
          "IsSuccessProperty": "isSuccess"
        }
      ]
    }
  ],

  "PerformanceCounterMetrics": [
    {
      "MetricName": "ActorServiceProcessorTimePercent",
      "CounterCategory": "Process",
      "CounterName": "% Processor Time",
      "CollectionIntervalInSeconds": "30",
      "ProcessIdCounterName": "ID Process"
    },
    {
      "MetricName": "ActorServicePercentTimeInGC",
      "CounterCategory": ".NET CLR Memory",
      "CounterName": "% Time in GC",
      "CollectionIntervalInSeconds": "30",
      "ProcessIdCounterName": "Process ID",
      "ProcessIdCounterCategory": ".NET CLR Memory"
    },
    {
      "MetricName": "ActorServiceExceptionsThrownPerSecond",
      "CounterCategory": ".NET CLR Exceptions",
      "CounterName": "# of Exceps Thrown / sec",
      "CollectionIntervalInSeconds": "30",
      "ProcessIdCounterName": "Process ID",
      "ProcessIdCounterCategory": ".NET CLR Memory"
    },
    {
      "MetricName": "ActorServiceProcessPrivateBytes",
      "CounterCategory": "Process",
      "CounterName": "Private Bytes",
      "CollectionIntervalInSeconds": "30",
      "ProcessIdCounterName": "ID Process"
    }
  ],

  "ApplicationInsightsSender": {
    "InstrumentationKey": "***REMOVED***"
  }
}
